<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
        "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>

<!-- bean definitions here -->

<bean id = "list" class = "com.capgemini.spring.DegreedAssignment2.Class">
	<property name = "lst">
		<list>
			<value>1</value>
			<value>2</value>
			<value>3</value>
		</list>
	</property>
</bean>
<bean id="set" class="com.capgemini.spring.DegreedAssignment2.Class">
	<property name = "set">
		<set>
			<value>4</value>
			<value>5</value>
			<value>6</value>
		</set>
	</property>
</bean>
<bean id="map" class="com.capgemini.spring.DegreedAssignment2.Class">
	<property name = "map">
		<map>
			<entry key="0" value="7"/>
			<entry key="1" value="8"/>
			<entry key="2" value="9"/>
		</map>
	</property>
</bean>
</beans>



